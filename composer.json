{
    "name": "innmind/stream",
    "type": "library",
    "description": "Wrapper to PHP stream functions",
    "keywords": ["stream"],
    "homepage": "http://github.com/Innmind/Stream",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/Stream/issues"
    },
    "require": {
        "php": "~7.4|~8.0",
        "innmind/immutable": "~3.0",
        "innmind/time-continuum": "~2.0",
        "innmind/url": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Stream\\": "src/",
            "Fixtures\\Innmind\\Stream\\": "fixtures/",
            "Properties\\Innmind\\Stream\\": "properties/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Stream\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "symfony/process": "^4.0",
        "vimeo/psalm": "^4.1",
        "innmind/black-box": "^4.8",
        "innmind/coding-standard": "^1.1"
    }
}
