{
    "name": "innmind/stream",
    "type": "library",
    "description": "Wrapper to PHP stream functions",
    "keywords": ["stream"],
    "homepage": "http://github.com/Innmind/Stream",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/Stream/issues"
    },
    "require": {
        "php": "~8.1",
        "innmind/immutable": "~4.0",
        "innmind/time-continuum": "~3.0",
        "innmind/url": "~4.0",
        "psr/log": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Stream\\": "src/",
            "Fixtures\\Innmind\\Stream\\": "fixtures/",
            "Properties\\Innmind\\Stream\\": "properties/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Stream\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "symfony/process": "~6.0",
        "vimeo/psalm": "^4.21",
        "nikic/php-parser": "^4.13.2",
        "innmind/black-box": "^4.8",
        "innmind/coding-standard": "~2.0"
    }
}
